@model List<QuestionnarieApp_.ViewModels.ManageUserViewModel>

<h2>Manage Users</h2>

<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>
                    <a asp-action="EditRoles" asp-route-userId="@user.Id" class="btn btn-primary">Edit Roles</a>
                </td>
            </tr>
        }
    </tbody>
</table>